<?xml version="1.0" encoding="UTF-8"?>
	<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		id        = "com.urbisclub"
		versionCode="10"
		version   = "1.2.0">
		<name>Urbisclub</name>
		<description>
Sorteos y promociones Urbisclub.		</description>
		<author href="http://www.casualiswebs.com" email="info@casualiswebs.com">
Casualis Webs
		</author>
		<preference name="phonegap-version" value="3.4.0" />
		<preference name="orientation" value="portrait" />
		
		<!--Retirar barra de arriba en IOS7-->
		<gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance">
		<false/>
		</gap:config-file>
		<feature name="StatusBar">
				<param name="ios-package" value="CDVStatusBar" onload="false" />
		</feature>
		
		<preference name="fullscreen" value="true" />
		<preference name="target-device" value="universal" />
		<preference name="webviewbounce" value="true" />
		<preference name="prerendered-icon" value="true" />
		<preference name="stay-in-webview" value="false" />
		<preference name="ios-statusbarstyle" value="default" />
		<preference name="detect-data-types" value="true" />
		<preference name="exit-on-suspend" value="false" />
		<preference name="show-splash-screen-spinner" value="true" />
		<preference name="auto-hide-splash-screen" value="true" />
		<preference name="EnableViewportScale" value="false" />
		<preference name="MediaPlaybackRequiresUserAction" value="false" />
		<preference name="AllowInlineMediaPlayback" value="false" />
		<preference name="BackupWebStorage" value="cloud" />
		<preference name="TopActivityIndicator" value="gray" />
		<preference name="KeyboardDisplayRequiresUserAction" value="true" />
		<preference name="HideKeyboardFormAccessoryBar" value="false" />
		<preference name="SuppressesIncrementalRendering" value="false" />
		<preference name="android-minSdkVersion" value="7" />
		<preference name="android-installLocation" value="auto" />
		<preference name="SplashScreenDelay" value="10000" />
		<preference name="ErrorUrl" value=""/>
		<preference name="BackgroundColor" value=""/>
		<preference name="KeepRunning" value="true"/>
		<preference name="DisallowOverscroll" value="true"/>
		<preference name="LoadingDialog" value=","/> 
		<preference name="LoadUrlTimeoutValue" value="50000" />
		<preference name="disable-cursor" value="true" />
		<gap:platform name="ios" />
		<gap:platform name="android" />
		<feature name="http://api.phonegap.com/1.0/geolocation"/>
		<feature name="http://api.phonegap.com/1.0/network"/>
		<feature name="http://api.phonegap.com/1.0/notification"/>
		<gap:plugin name="org.apache.cordova.device"  />
		<gap:plugin name="org.apache.cordova.device-motion"  />
		<gap:plugin name="org.apache.cordova.device-orientation" />
		<gap:plugin name="org.apache.cordova.dialogs"  />
		<gap:plugin name="org.apache.cordova.file"  />
		<gap:plugin name="org.apache.cordova.geolocation"  />
		<gap:plugin name="org.apache.cordova.inappbrowser"  />
		<gap:plugin name="org.apache.cordova.network-information"  />
		<gap:plugin name="org.apache.cordova.splashscreen"  />
		<gap:plugin name="org.apache.cordova.vibration"  />
		<gap:plugin name="de.appplant.cordova.plugin.local-notification" />
		<icon src="images/iconos/ios/icon.png" gap:platform="ios" width="57" height="57" />
		<icon src="images/iconos/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
		<icon src="images/iconos/ios/icon_at_2x.png" gap:platform="ios" width="114" height="114" />
		<icon src="images/iconos/android/144.png" gap:platform="ios" width="144" height="144" />
		<icon src="images/iconos/ios/icon_76.png" gap:platform="ios" width="76" height="76" />
		<icon src="images/iconos/ios/icon_120.png" gap:platform="ios" width="120" height="120" />
		<icon src="images/iconos/ios/icon_144.png" gap:platform="ios" width="152" height="152" />
		<icon src="images/iconos/android/ldpi.png" gap:platform="android" gap:density="ldpi" />
		<icon src="images/iconos/android/mdpi.png" gap:platform="android" gap:density="mdpi" />
		<icon src="images/iconos/android/hdpi.png" gap:platform="android" gap:density="hdpi" />
		<icon src="images/iconos/android/xhdpi.png" gap:platform="android" gap:density="xhdpi" />
		<icon src="images/iconos/android/144.png" gap:platform="android" gap:density="xxhdpi" />
		<icon src="icon.png" />
		<gap:splash src="images/splash/ios/Default.png" gap:platform="ios" width="320" height="480" />
		<gap:splash src="images/splash/ios/Default_at_2x.png" gap:platform="ios" width="640" height="960" />
		<gap:splash src="images/splash/ios/Default_iphone5.png" gap:platform="ios" width="640" height="1136" />
		<gap:splash src="images/splash/ios/Default-Portrait.png" gap:platform="ios" width="768" height="1024 " />
		<gap:splash src="images/splash/ios/Default-Landscape.png" gap:platform="ios" width="1024" height="768" />
		<gap:splash src="images/splash/android/ldpi.png" gap:platform="android" gap:density="ldpi" />
		<gap:splash src="images/splash/android/mdpi.png" gap:platform="android" gap:density="mdpi" />
		<gap:splash src="images/splash/android/hdpi.png" gap:platform="android" gap:density="hdpi" />
		<gap:splash src="images/splash/android/xhdpi.png" gap:platform="android" gap:density="xhdpi" />
		<access origin="*.urbisclub.com" />
		<access origin="http://www.urbisclub.com" />
		<access origin="*" />
		<access origin="*.google.com" />
		<access origin="*.googleapis.com" />
		<access origin="*.gstatic.com" />
		<access origin="*.twitter.com" />
		<access origin="*.twitter.es" />
		<access origin="*.pushwoosh.com" />
		<gap:plugin name="com.phonegap.plugins.childbrowser" />
		<gap:plugin name="com.phonegap.plugins.facebookconnect" >
			<param name="APP_ID" value="306068086161250" /> 
			<param name="APP_NAME" value="Sorteos y promociones Urbisclub" /> 
		</gap:plugin>
		<gap:plugin name="de.appplant.cordova.plugin.local-notification"  />
		<gap:plugin name="com.phonegap.plugin.statusbar"  />
		<gap:plugin name="com.phonegap.plugins.pushplugin"  />
	</widget>